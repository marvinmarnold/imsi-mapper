<%- model_class = ImsiDatum -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:aimsicd_threat_level) %>:</strong></dt>
  <dd><%= @imsi_datum.aimsicd_threat_level %></dd>
  <dt><strong><%= model_class.human_attribute_name(:latitude_degrees) %>:</strong></dt>
  <dd><%= @imsi_datum.latitude_degrees %></dd>
  <dt><strong><%= model_class.human_attribute_name(:longitude_degrees) %>:</strong></dt>
  <dd><%= @imsi_datum.longitude_degrees %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              imsi_data_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_imsi_datum_path(@imsi_datum), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              imsi_datum_path(@imsi_datum),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
