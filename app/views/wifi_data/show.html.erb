<%- model_class = WifiDatum -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:num_wifi_hotspots) %>:</strong></dt>
  <dd><%= @wifi_datum.num_wifi_hotspots %></dd>
  <dt><strong><%= model_class.human_attribute_name(:latitude_degrees) %>:</strong></dt>
  <dd><%= @wifi_datum.latitude_degrees %></dd>
  <dt><strong><%= model_class.human_attribute_name(:longitude_degrees) %>:</strong></dt>
  <dd><%= @wifi_datum.longitude_degrees %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              wifi_data_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_wifi_datum_path(@wifi_datum), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              wifi_datum_path(@wifi_datum),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
